{
  "expo": {
    "name": "FirebaseAuth",
    "slug": "FirebaseAuth",
    "version": "1.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.minston.FirebaseAuth"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.minston.FirebaseAuth"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "7e01f9b7-e48e-41d0-99c5-2f91413a7c6b"
      }
    },
    "fonts": [
      {
        "name": "Lato",
        "asset": "./assets/fonts/Lato.ttf"
      }
    ]
  }
}
